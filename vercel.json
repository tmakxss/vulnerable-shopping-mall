{
  "version": 2,
  "functions": {
    "run.py": {
      "runtime": "@vercel/python"
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "run.py"
    }
  ],
  "env": {
    "FLASK_ENV": "production",
    "PYTHONPATH": "."
  },
  "build": {
    "env": {
      "FLASK_ENV": "production"
    }
  }
}